# del command decProjectMaterialStatusLink;
add command decProjectMaterialStatusLink
	label 'ProgramCentral.Label.Procurement_Schedule_-_Material_Status'
	href '${COMMON_DIR}/emxIndentedTable.jsp?table=decMaterialStatusSummary&program=emxProjectSpace:getMaterialStatusList&header=ProgramCentral.Label.Procurement_Schedule_-_Material_Status&editLink=true&parallelLoading=true&maxCellsToDraw=2000&scrollPageSize=50&pageSize=300&showPaginationToggle=false&decExport=true&exportMode=MaterialStatus'
	setting 'Target Location' 'content'
	setting 'Registered Suite' 'ProgramCentral'
;

mod command decProjectMaterialStatusLink
	#add setting 'Access Expression' 'false'
	remove setting 'Access Expression'
;

# Modified by hslee on 2023.07.31
mod command decProjectMaterialStatusLink
	add setting 'Access Expression' 'type == "Project Space" && attribute[decProjectType] == ongoing' 
;

