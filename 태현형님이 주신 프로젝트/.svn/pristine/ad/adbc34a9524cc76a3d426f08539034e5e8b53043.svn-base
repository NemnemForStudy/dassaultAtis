	public MapList documentFilterProcess(Context context, String[] args, StringBuffer sbWhere, MapList ml, Map fMap) throws Exception {
		Map programMap = JPO.unpackArgs(args);
		String getTable = (String) programMap.get("table");
		Map<String, String> filteredMap = fMap;
