/* TEMP */
.sn-search-field.uwa-autocomplete.nv-autocomplete-container {
  /* display: block !important; */
}
/* TEMP */

.sn-search-field.uwa-autocomplete.nv-autocomplete-container {
  float: left;

}

/* CASE FOR UNDER 1000px */
@media screen and (max-height: 1000px) {
  .sn-search-field.uwa-autocomplete.nv-autocomplete-container {
    height: 75vh;
  }
}

/* CASE FOR ABOVE 1000px */
@media screen and (min-height: 1000px) {
  .sn-search-field.uwa-autocomplete.nv-autocomplete-container {
    height: 700px;
  }
}

.sn-search-field.uwa-autocomplete.nv-autocomplete-container.autocomplete-hidden {
  display: none;
}

.search-global-content-container,
.search-global-tab-container {
  width: 250% !important;
  left: 50%;
  transform: translate(-30%,0%);
  background-color: #fff;
  box-shadow: 5px 6px 10px 0px #d1d4d4;
}

.search-global-content-container {
  height: 100%;
  box-shadow: 5px 6px 10px 0px #d1d4d4;
  border-left: 1px solid #D1D4D4;
  border-right: 1px solid #D1D4D4;
  border-top: 1px solid #D1D4D4;
}

.button-container {
  border-left: 1px solid #D1D4D4;
  border-right: 1px solid #D1D4D4;
  height: 10%;
}

.search-tab-advanced-search-button,
.search-tab-expert-search-button,
.search-tab-favorite-search-button {
  width: 100%;
  height: 80%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 0.9em;
  font-weight: bold;
  border: 1px grey solid;
}

.search-tab-advanced-search-button:hover,
.search-tab-expert-search-button:hover,
.search-tab-favorite-search-button:hover {
  cursor: pointer;
  background-color: #D1D4D4;
}

.search-tab-advanced-search-button > i,
.search-tab-expert-search-button > i,
.search-tab-favorite-search-button > i {
  margin-right: 1em;
}

.search-tab-advanced-search-button {
  margin-left: 2%;
  margin-right: 1%;
}

.search-tab-expert-search-button {
  margin-left: 1%;
  margin-right: 1%;
}

.search-tab-favorite-search-button {
  margin-left: 1%;
  margin-right: 2%;
}

.search-global-tab-container {
  height: 5%;
  display: inline-table;
  border-left: 1px solid #D1D4D4;
  border-right: 1px solid #D1D4D4;
  border-bottom: 1px solid #D1D4D4;
}

.instant-search-container {
  height: 96%;
  width: 60%;
  float: right;
  margin-top: 2%;
  margin-bottom: 2%;
  padding-right: 2%;
  overflow-y: scroll;
}

.mycontent-container {
  height: 100%;
  width: 60%;
  float: right;
  padding-left: 15px;
  padding-right: 15px;
  overflow-y: scroll;
}

.mycontent-empty-view {
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.section-container-spinner-container {
  /* height: 100%; */
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 2%;
  margin-top: 2%;
}

/****************/
/* SECTION TILE */
/****************/
.section-container {
  display: flex;
  flex-direction: column;
}

.section-container-header {
  background-color: white;
  border-radius: 28px;
  float: left;
  width: 100%;
  margin-bottom: 2%;
}

.section-container-icon,
.section-container-title {
  float: left !important;
  margin-right: 0px !important;
}

.section-container-title {
  margin-left: 15px !important;
}

.section-container-icon {
  margin-left: 30px !important;
}

.section-container-icon {
  height: 32px;
  width: 32px;
  font-size: 16px;
  line-height: 44px;
  background-position: center center;
  float: left;
  margin-top: 6px !important;
  margin-bottom: 6px !important;
  display: flex !important;
  align-items: center;
  place-content: center;
  background-color: #e2e4e3;
  border-radius: 4px;
  background-size: 100%;
  background-repeat: no-repeat;
  background-position: center;
}

.section-container-link {
  float: right !important;
  margin-right: 20px;
  color: #368EC4;
}

.section-container-link:hover {
  cursor: pointer;
  text-decoration: underline;
  color: #005686;
}

.section-object {
  display: flex;
  height: 50px;
  background-color: white;
  margin-left: 40px;
  margin-right: 10px;
  margin-bottom: 2%;
}

.section-object-image {
  height: 50px;
  width: 20%;
  border: 4px solid white;
  background-size: 100%;
  background-repeat: no-repeat;
  background-position: center;
  background-color: lightgrey;
}

.section-object-content {
  height: 50px;
  width: 80%;
  padding-left: 5px;
  display: flex;
  flex-direction: column;
  justify-content: space-evenly;
}

.section-object-title,
.section-object-description {
  line-height: normal;
  overflow: hidden;
}

.section-object-title {
  display: flex;
}

.section-object-description {
  text-overflow: ellipsis;
  font-size: 11px;
  color: #77797c;
}

.section-object-content-icon {
  width: 16px;
  height: 16px;
  margin-right: 5px;
}

.section-object-content-text {
  font-size: 13px;
  color: #368ec4;
  text-overflow: ellipsis;
  overflow: hidden;
}

.section-object-content-text:hover {
  color: #42A2DA;
  text-decoration: underline;
  cursor: pointer;
}

/*********************/
/* SECTION THUMBNAIL */
/*********************/
.section-container-content {
  display: flex;
  flex-wrap: wrap;
  padding-left: 10%;
}

.section-thumbnail {
  width: 33%;
}

.section-thumbnail-content {
  display: flex;
  flex-direction: column;
}

.section-thumbnail-content:hover {
  background-color: #D1D4D4;
  cursor: pointer;
}

.section-thumbnail-image {
  margin: auto;
  max-height: 64px;
}

.section-thumbnail-title {
  display: flex;
  align-items: center;
  justify-content: center;
  padding-right: 5px;
  padding-left: 5px;
}

.section-thumbnail-content-icon {
  height: 14px;
}

.section-thumbnail-content-text {
  text-overflow: ellipsis;
  overflow: hidden;
  padding-left: 5px;
}
/*********************/

.history-container,
.suggestion-container{
  width: 40%;
  float: left;
  background: white;
  background-clip: content-box;
  position: relative;
  display: flex;
  flex-direction: column;
}

.history-container {
  height: 100%;
}

.suggestion-container {
  height: 100%;
  position: relative;
  padding: 15px;
}

.mycontent-empty-view,
.instant-search-empty-view {
  display: flex;
  height: 85%;
  width: 100%;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}

.mycontent-empty-view-image,
.instant-search-empty-view-image {
  font-size: 100px;
  height: 100px;
  position: relative !important;
  display: flex;
  align-items: center;
}

.instant-search-empty-view-icon {
  color: #00B8DE;
  position: absolute;
  top: 0px;
  right: -25px;
  font-size: 40px;
}

.mycontent-empty-view-text,
.instant-search-empty-view-text {
  font-weight: bold;
  font-size: 18px;
}

.mycontent-empty-view-subtext,
.instant-search-empty-view-subtext {
  white-space: normal;
  line-height: normal;
  text-align: center;
}

.mycontent-title-container {
  height: 8%;
  min-height: 44px;
  border-bottom: 1px solid black;
  display: flex;
  align-items: center;
}

.mycontent-icon {
  margin-left: 5% !important;
  margin-right: 5% !important;
}

.mycontent-link {
  margin-left: auto !important;
  margin-right: 5% !important;
  color: #368ec4;
}

.mycontent-link:hover {
  cursor: pointer;
  color: #005686;
}

.search-tab-container,
.settings-tab-container,
.predefined-queries-tab-container,
.instant-search-tab-container {
  height: 100%;
  background: #F1F1F1;
}

.predefined-queries-tab-container {
  margin-top: 5px;
  display: flex;
  flex-direction: column;
}

.search-tab-buttons-container {
  height: 10%;
  float: left;
  width: 100%;
  display: flex;
  align-items: center;
}

.search-dropdown-tab-mycontent,
.search-dropdown-tab-favorite-searches,
.search-dropdown-tab-settings,
.search-dropdown-tab-predefined-queries {
  height: 100%;
  float: right;
  display: flex;
  align-items: center;
  padding-right: 1em;
  border-bottom: 3px solid transparent;
  margin-right: 1em;
}

.search-dropdown-tab-mycontent:hover,
.search-dropdown-tab-favorite-searches:hover,
.search-dropdown-tab-settings:hover,
.search-dropdown-tab-predefined-queries:hover {
  cursor: pointer;
  border-bottom: 3px solid grey;
  transition: border-bottom-color .2s ease-in-out,margin .2s ease-in-out;
}

.tab-selected, .tab-selected:hover {
  border-bottom: 3px solid #368ec4;
  cursor: auto;
}

.tab-selected > .search-dropdown-tab-icon {
  color: #368ec4;
}

.search-dropdown-clear-history,
.search-dropdown-server-suggestions {
  float: left;
  display: flex;
  padding-left: 1%;
  width: 100%;
}

.search-dropdown-clear-history-text,
.search-dropdown-server-suggestions-text {
  width: 100%;
  display: flex;
  justify-content: space-between;
  margin-left: 10px;
  font-weight: bold;
}

.search-dropdown-clear-history-icon {
  margin-right: 10px;
}

.search-dropdown-clear-history-icon:hover {
  color: #368ec4;
  cursor: pointer;
}

.suggestion-container-empty {
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.suggestion-container-empty-icon {
  font-size: 64px;
  height: 64px;
}

.suggestion-container-empty-text {
  font-weight: bold;
}

/* .search-dropdown-clear-history:hover .search-dropdown-clear-history-icon,
.search-dropdown-clear-history:hover .search-dropdown-clear-history-text {
  color: #368ec4;
  cursor: pointer;
} */

.search-dropdown-tab-icon {
  margin-left: 0.8em !important;
  margin-right: 0.8em !important;
}

.search-dropdown-tab-text {
  overflow: hidden;
  text-overflow: ellipsis;
}

.search-tab-content {
  height: 90%;
  display: flex;
  flex-direction: row;
  padding: 15px;
  overflow-y: hidden;
}

.search-history-container {
  height: 44px;
  width: 100%;
  display: flex;
  align-items: center;
}

.history-container-content {
  width: 100%;
  overflow-y: scroll;
}

.history-container-content::-webkit-scrollbar,
.mycontent-container::-webkit-scrollbar,
.instant-search-container::-webkit-scrollbar,
.predefined-queries-content::-webkit-scrollbar,
.predefined-queries-tab-container .wux-controls-expander-toggle .wux-controls-expander-body-toggle::-webkit-scrollbar {
  width: 7px;
}

.history-container-content::-webkit-scrollbar-thumb,
.mycontent-container::-webkit-scrollbar-thumb,
.instant-search-container::-webkit-scrollbar-thumb,
.predefined-queries-content::-webkit-scrollbar-thumb,
.predefined-queries-tab-container .wux-controls-expander-toggle .wux-controls-expander-body-toggle::-webkit-scrollbar-thumb {
  background-color: #e4e6ea;
  border-radius: 0.25em;
}

.search-history-delete {
  font-size: 16px;
  margin-left: auto !important;
  margin-right: 10px !important;
  display: none !important;
}

.search-history-container:hover {
  background-color: #D1D4D4;
  cursor: pointer;
}

.search-history-container:hover > .search-history-delete {
  display: block !important;
}

.search-history-icon {
  font-size: 16px;
  margin-left: 10px !important;
  margin-right: 10px !important;
}

.search-history-text {
  max-width: 75%;
  overflow: hidden;
  text-overflow: ellipsis;
}

.search-history-text-filtered {
  font-weight: bold;
}

.search-history-delete:hover {
  cursor: pointer;
}

.settings-container {
  height: 100%;
  padding: 3%;
}

.setting-container {
  height: 44px;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.setting-container .option-container,
.setting-container .option-container-select {
  max-height: 75%;
  height: 42px;
  width: 60%;
  float: right;
  display: flex;
}

.setting-container .option-container-select {
  display: flex;
}

.setting-container .option-container input {
  width: 15px;
  margin-right: 15px;
  margin-top: 0px !important;
  height: 100%;
}

.setting-container .option-container span {
  display: flex;
  align-items: center;
}

.setting-container .option-container span.disabled {
  color: #b4b6ba;
}

.setting-container .option-container-select select {
  width: 100%;
  border: #D1D4D4 1px solid;
  border-radius: 5px;
}

.search-global-content-container .settings-tab-container .settings-container .apply-button {
  position: absolute;
  bottom: 3%;
  right: 3%;
}

.search-disabled {
  color: #D1D4D4;
}

.search-disabled:hover,
.search-disabled:hover > div,
.search-disabled:hover > span {
  color: #D1D4D4 !important;
  cursor: unset !important;
}

/*********************/
/*     HELP TAB      */
/*********************/

.predefined-queries-tab-container .special-characters-link-container {
  display: flex;
  padding-right: 3%;
  justify-content: flex-end;
}

.predefined-queries-tab-container .special-characters-link-container .wux-controls-button {
  height: 32px;
}

.predefined-queries-tab-container .wux-controls-abstract {
  margin-bottom: 5px;
  margin-top: 5px;
}

.predefined-queries-tab-container .wux-controls-abstract.expanded {
  height: 80% !important;
}

.predefined-queries-tab-container .wux-controls-abstract .wux-controls-expander-container {
  height: 100%;
  padding-left: 3%;
  padding-right: 3%;
}

.predefined-queries-tab-container .wux-controls-abstract .wux-controls-expander-container .wux-controls-expander-body-toggle {
  height: 100%;
}

.predefined-queries-tab-container .wux-controls-abstract.special-characters-container .wux-controls-expander-container .wux-controls-expander-body-toggle {
  overflow-y: scroll;
}

.predefined-queries-tab-container .wux-controls-abstract.type-shortcuts-container .wux-controls-expander-container .wux-controls-expander-body-toggle {
  overflow-y: hidden;
}

.predefined-queries-tab-container .wux-controls-abstract .wux-controls-expander-container .wux-controls-expander-body-toggle .predefined-queries-documentation-container .predefined-queries-table {
  margin-bottom: 24px;
}

.predefined-queries-documentation-container table {
  width: 100%
}

.predefined-queries-documentation-container table tbody tr td {
  border: 1px solid grey;
  padding: 5px;
  line-height: 16px;
}

.predefined-queries-documentation-container table tbody tr td:first-of-type {
  width: 15%;
}

.predefined-queries-documentation-header {
  font-weight: bold
}

.predefined-queries-documentation-content {
  background-color: white;
}

.predefined-queries-documentation-container table tbody tr td ul {
  list-style-type: disc;
}

.predefined-queries-documentation-container table tbody tr td ul li {
  list-style-type: 100%;
}

.predefined-queries-documentation-container table tbody tr td p.important{
  border: 1px solid grey;
  padding: 3px;
  width: fit-content;
}

.type-shortcuts-content {
  height: 100%;
}

.predefined-queries-content {
  height: 90%;
  overflow-y: scroll;
}

.predefined-queries-tab-container .collapsed {
  height: auto !important;
  overflow-y: hidden;
}

/***** PQ Component *****/

.predefined-queries-container .autocomplete {
  height: 10%;
  margin-left: 2%;
  margin-right: 2%;
}

.predefined-queries-container .autocomplete input {
  border: transparent;
  margin: unset;
}

.predefined-queries-input .autocomplete-searchbox {
  background-color: #F1F1F1;
  border: none;
  padding-left: 0px;
  padding-right: 0px;
}

.predefined-queries-input .autocomplete-searchbox input {
  width: 100%;
  margin-top: 0px;
  background-color: white;
}

.predefined-queries-input .autocomplete-searchbox span {
  position: absolute;
  right: 9px; /* 50% font-size */
  /* top: 9px; /* 50% font-size */
}

.predefined-queries-content .predefined-queries-table {
  width: 100%;
}

.predefined-queries-table-body {
  margin-left: 2%;
  margin-right: 2%;
}

.predefined-queries-grey {
  background: rgb(240, 240, 240);
}

.predefined-queries-white {
  background: rgb(255, 255, 255);
}

.predefined-queries-grey table,
.predefined-queries-white table {
  width: 100%;
}

.predefined-queries-content-left {
  float: left;
}

.predefined-queries-content-right {
  float: right;
}

.predefined-queries-content-abbreviation:hover {
  cursor: pointer;
}

.predefined-queries-table tbody tr {
  line-height: 2;
  overflow-x: hidden;
}
/*********************/
