################################################################################################
# Create By thok 2023. 07. 06
# Admin Type : command
# Name : decRequestAccept
################################################################################################
#del command 'decRequestAccept';
add command decRequestAccept
	description 'Request Accept'
	label 'emxProgramCentral.Label.RequestAccept'
	href '${COMMON_DIR}/decRequestStatus.jsp?mode=accept'
	alt 'emxProgramCentral.Label.RequestAccept'
	setting 'Image' '${COMMON_DIR}/images/iconActionApprove.png'
	setting 'Confirm Message' 'emxProgramCentral.Alarm.RequestAccept' 
	setting 'Registered Suite' 'ProgramCentral'
	setting 'Target Location' 'content'
	setting 'Submit' 'true'
;
add property 'command_decRequestAccept' on prog eServiceSchemaVariableMapping.tcl to command 'decRequestAccept'
;
# Modified by thok 2023.07.14
mod command decRequestAccept
	add setting 'Access Expression' 'context.user.isassigned[decSystemAdmin]'
;

# Modified by thok 2023.08.09
mod command decRequestAccept
	remove setting 'Access Expression'
;

# Modified By thok : 2023. 08. 10
mod command 'decRequestAccept' 
	add setting 'Access Function' 'isRequestAcceptAvailable' 
	add setting 'Access Program' 'decRequest'
	add setting 'Image' '${COMMON_DIR}/images/iconActionAcceptTask.png'
;

# Modified By thok : 2023. 08. 10
mod command decRequestAccept
	add setting 'Target Location' 'pagehidden'
;