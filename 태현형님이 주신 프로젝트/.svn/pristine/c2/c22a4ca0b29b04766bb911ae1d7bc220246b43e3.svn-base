/* -------------------------------------------------------------------------
   Reset
   ------------------------------------------------------------------------- */
			.highcharts-credits {
			    display: none;
			  }
			
			  .custom-button {
			    position: absolute;
			    top: 10px;
			    right: 10px;
			
			    border: 1px solid #CCCCCC;
			    border-radius: 3px;
			    padding: 5px 10px;
			    cursor: pointer;
			  }
			  thead {

				  border-color: black;
				
				}
			   table {
				  border-spacing: 3px;
				  border-collapse: separate;
				}
				#double {
				   border: double 10px;
				}
				
			  #container {
			    margin-right: 20px;
			    height: 100%; /* 적절한 높이 설정 */
			    display: flex;
			    flex-direction: column;
			    justify-content: space-between;
			  }
			  
			  .highcharts-background {
				  fill: none; /* 바탕색을 파란색으로 변경 */
				}
				
				
				
				