#del command 'decRequestDelete';
add command decRequestDelete
	description 'Request Delete'
	label 'emxProgramCentral.Label.RequestDelete'
	href '${COMMON_DIR}/decRequestStatus.jsp?mode=delete'
	alt 'emxProgramCentral.Label.RequestDelete'
	setting 'Image' '${COMMON_DIR}/images/iconActionDelete.png'
	setting 'Confirm Message' 'emxProgramCentral.Alarm.RequestDelete' 
	setting 'Registered Suite' 'ProgramCentral'
	setting 'Target Location' 'content'
	setting 'Submit' 'true'
;
add property 'command_decRequestDelete' on prog eServiceSchemaVariableMapping.tcl to command 'decRequestDelete'
;
# Modified by thok 2023.07.14
mod command decRequestDelete
	add setting 'Access Expression' 'context.user.isassigned[decSystemAdmin]'
;

# Modified by thok 2023.08.09
mod command decRequestDelete
	remove setting 'Access Expression'
;